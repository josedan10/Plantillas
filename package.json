{
  "name": "plantillas",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ajv-keywords": "^3.1.0",
    "ansi-html": "0.0.7",
    "array-includes": "^3.0.3",
    "debug": "^3.1.0",
    "html-entities": "^1.2.1",
    "http-proxy-middleware": "^0.17.4",
    "import-local": "^1.0.0",
    "internal-ip": "^3.0.1",
    "ip": "^1.1.5",
    "react": "^16.2.0",
    "react-router": "^4.2.0",
    "react-scripts": "1.1.0",
    "sockjs": "^0.3.19",
    "sockjs-client": "^1.1.4",
    "webpack": "^4.0.0-beta.1",
    "webpack-dev-middleware": "^3.0.0",
    "webpack-log": "^1.1.2"
  },
  "scripts": {
    "start": "webpack",
    "test:watch": "webpack --watch",
    "start:dev": "webpack-dev-server",
    "build:dev": "webpack --config ./webpack.config.js --progress --colors",
    "init-dev-server": "webpack-dev-server --config ./webpack.config.js --progress --colors",
    "server": "node server.js",
    "beautify": "npm run lint -- --fix",
    "ci": "npm run cover -- --report lcovonly && npm run test",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha",
    "lint": "eslint bin lib test examples client-src",
    "mocha": "mocha --full-trace --check-leaks",
    "prepublish": "(rm ssl/*.pem || true) && npm run -s transpile:index && npm run -s build:live && npm run -s build:index && npm run -s build:sockjs",
    "test": "npm run lint && npm run mocha",
    "transpile:index": "babel client-src/default --out-dir client --ignore *.config.js",
    "build:index": "webpack ./client-src/default/index.js -o client/index.bundle.js --color --config client-src/default/webpack.config.js",
    "build:live": "webpack ./client-src/live/index.js -o client/live.bundle.js --color --config client-src/live/webpack.config.js",
    "build:sockjs": "webpack ./client-src/sockjs/index.js -o client/sockjs.bundle.js --color --config client-src/sockjs/webpack.config.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.2",
    "eslint-config-webpack": "^1.2.5",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.4",
    "istanbul": "^0.4.5",
    "jquery": "^3.3.1",
    "less": "^3.0.1",
    "less-loader": "^4.0.6",
    "marked": "^0.3.17",
    "mocha": "^5.0.1",
    "mocha-sinon": "^2.0.0",
    "node-sass": "^4.7.2",
    "react-dom": "^16.2.0",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "sass-loader": "^6.0.7",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack-cli": "^2.0.10"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
